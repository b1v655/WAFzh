<Window x:Class="Order.WPF.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Order.WPF.View"
        mc:Ignorable="d"
        Title="Főmenü" Height="212.742" Width="300">
    <StackPanel>
        <Button Command="{Binding AddMenuItemCommand}" Width="150" Margin="20">Étel hozzáadása a menühöz</Button>
        <Button Command="{Binding OrdersCommand}" Width="150" Margin="20">Rendelések listázása</Button>
        <Button Command="{Binding LogoutCommand}" Width="150" Margin="20">Kijelentkezés</Button>
    </StackPanel>
</Window>
